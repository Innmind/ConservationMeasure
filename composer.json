{
    "name": "innmind/conservation-measure",
    "type": "library",
    "description": "High level abstraction to blur the line between remote and local filesystem",
    "keywords": ["filesystem", "file", "storage"],
    "homepage": "http://github.com/Innmind/ConservationMeasure",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/ConservationMeasure/issues"
    },
    "require": {
        "php": "~7.2",
        "innmind/stream": "^1.5",
        "innmind/immutable": "^2.15",
        "innmind/filesystem": "^3.3",
        "innmind/ipc": "^2.1",
        "innmind/cli": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\ConservationMeasure\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\ConservationMeasure\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "giorgiosironi/eris": "^0.11.0"
    }
}
